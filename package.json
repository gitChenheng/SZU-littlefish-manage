{
    "private": true,
    "scripts": {
        "start": "umi dev",
        "dev": "umi dev",
        "build": "cross-env UMI_ENV=dev umi build",
        "build:prod": "cross-env UMI_ENV=prod umi build",
        "postinstall": "umi generate tmp",
        "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
        "test": "umi-test",
        "test:coverage": "umi-test --coverage",
        "serve": "serve ./dist"
    },
    "betterScripts": {},
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,less,md,json}": [
            "prettier --write"
        ],
        "*.ts?(x)": [
            "prettier --parser=typescript --write"
        ]
    },
    "dependencies": {
        "@ant-design/pro-layout": "^5.0.12",
        "@umijs/plugin-sass": "^1.1.1",
        "@umijs/preset-react": "1.x",
        "@umijs/test": "^3.2.14",
        "axios": "^0.19.2",
        "cross-env": "^7.0.2",
        "lint-staged": "^10.0.7",
        "lodash": "^4.17.20",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "tslint": "^6.1.3",
        "typescript": "^3.9.7",
        "umi": "^3.2.14",
        "xlsx": "^0.16.5",
        "yorkie": "^2.0.0"
    }
}
